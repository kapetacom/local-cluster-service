{
  "name": "@kapeta/local-cluster-service",
  "version": "0.1.0",
  "description": "Manages configuration, ports and service discovery for locally running Kapeta systems",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kapetacom/local-cluster-service.git"
  },
  "keywords": [
    "kapeta"
  ],
  "author": "Henrik Hofmeister <hh@kapeta.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kapetacom/local-cluster-service/issues"
  },
  "scripts": {
    "start": "nodemon start.js",
    "test": "echo its fine"
  },
  "homepage": "https://github.com/kapetacom/local-cluster-service#readme",
  "dependencies": {
    "@kapeta/codegen": "<2",
    "@kapeta/local-cluster-config": "<2",
    "@kapeta/nodejs-api-client": "<2",
    "@kapeta/nodejs-registry-utils": "<2",
    "@kapeta/nodejs-utils": "<2",
    "@kapeta/sdk-config": "<2",
    "express": "4.17.1",
    "express-promise-router": "^4.1.1",
    "fs-extra": "^11.1.0",
    "glob": "7.1.6",
    "lodash": "^4.17.15",
    "md5": "2.2.1",
    "mkdirp": "0.5.1",
    "node-docker-api": "1.1.22",
    "node-uuid": "^1.4.8",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "socket.io": "^4.5.2",
    "yaml": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
